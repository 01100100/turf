{
  "private": true,
  "version": "2.0.2",
  "description": "a node.js library for performing geospatial operations with geojson",
  "scripts": {
    "test": "lerna bootstrap && lerna run test"
  },
  "eslintConfig": {
    "rules": {
      "quotes": 0,
      "curly": 0,
      "camelcase": 0,
      "space-infix-ops": 0,
      "comma-spacing": 0,
      "no-use-before-define": 0,
      "strict": 0
    },
    "env": {
      "node": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/turfjs/turf.git"
  },
  "keywords": [
