If Right$(temp1$, 1) = "\" Then temp1$ = Left$(temp1$, Len(temp1$) - 1)
If Len(temp1$) > 3 Then temp1$ = temp1$ + "\"
Label1.Caption = temp1$
temp2$ = Label1.Caption

End Sub

