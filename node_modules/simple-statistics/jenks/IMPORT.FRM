Private Sub start_Click()

On Error Resume Next

file$ = Label1.Caption
dataname$ = Text1.Text

Open file$ For Input As #1

If Err Then
    MsgBox ("Problem accessing data.  Please fix it."), 5, "ERROR"
    Exit Sub
End If

i = 0
n = 0

While Not EOF(1)
    i = i + 1
    Input #1, nam$(i), value(i)
    sumx = sumx + value(i)
    sumxx = sumxx + value(i) ^ 2
    n = n + 1
Wend
Close #1

mean = sumx / n
sd = Sqr(((n * sumxx) - (sumx ^ 2)) / (n * (n - 1)))

Label2.Visible = -1
Label2.Caption = "Sorting . . ."

'******************sort into ascending order******

        yy = 1
13042   yy = 2 * yy
        If yy < n Then GoTo 13042
13044   yy = Int((yy - 1) / 2)
        If yy = 0 Then GoTo 13073
        it = n - yy
        For i = 1 To it
            j = i
13049       z = j + yy
            If value(z) <= value(j) Then GoTo 13054
13051   Next i
13052   GoTo 13044

13054   temp = value(z): value(z) = value(j): value(j) = temp
        tempor$ = nam$(z): nam$(z) = nam$(j): nam$(j) = tempor$
13057   j = j - yy
13058   If j > 0 Then GoTo 13049
13059   GoTo 13051
13073   Label2.Visible = 0
Screen.MousePointer = 0
minvalue = value(1)
maxvalue = value(n)
If minvalue < 0 Then adder = minvalue / 10 Else adder = 0.1

MsgBox "Data successfully imported.", 0, "OK"
importer.Hide
intro.view.Enabled = True
intro.Class.Enabled = True

End Sub

