Private Sub ok_Click()

classer.List1.Clear
classer.List2.Clear

List1.Clear
List2.Clear
List3.Clear
List4.Clear
List5.Clear
For i = 1 To n
    cl(i) = 0
Next

reporter.Hide
intro.Show

End Sub

Private Sub printer_Click()
 
Printer.Print dataname$

For i = 1 To numclass
    Printer.Print "Class"; i; c$; lowclass(i); c$; highclass(i)
Next

For i = 1 To n
    Printer.Print nam$(i); c$; value(i); c$; cl(i)
Next
'printer.EndDoc

End Sub

