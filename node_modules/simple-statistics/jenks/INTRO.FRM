Private Sub lister_Click()

listdata.Show

For i = 1 To n
    num$ = Str$(value(i))
    Text$ = nam$(i) + num$
    listdata.List1.AddItem Text$
Next

listdata.Label1.Caption = dataname$
listdata.Label6.Caption = Str$(minvalue)
listdata.Label7.Caption = Str$(maxvalue)
listdata.Label8.Caption = Str$(mean)
listdata.Label9.Caption = Str$(sd)

End Sub

