Private Sub import_Click()
  
MsgBox "You may only import comma-separated ASCII data files that begin with" + Chr(13) + "object names followed by numerical data values in the following format:" + Chr$(13) + Chr$(10) + Chr$(13) + Chr$(10) + "Arkansas, 458" + Chr$(13) + Chr$(10) + "Maine, 6732" + Chr$(13) + Chr$(10) + "Wisconsin, 23" + Chr$(13) + Chr$(10) + ". . ." + Chr$(13) + Chr$(10) + Chr$(13) + Chr$(10) + "Only 1 name and data value per line.", 48, "WARNING"
importer.Show

End Sub

