Attribute VB_Name = "user"
Attribute VB_Creatable = False
Attribute VB_Exposed = False
Private Sub cancel_Click()

List1.Clear
user.Hide

End Sub

Private Sub Form_Load()

Text1.Text = "0"
Text2.Text = "0"
Text1.Enabled = False
Text2.Enabled = False

End Sub

Private Sub List1_Click()

Text1.Text = Str$(lowclass(List1.ListIndex + 1))
Text2.Text = Str$(highclass(List1.ListIndex + 1))
Text1.Enabled = True
Text2.Enabled = True

End Sub

Private Sub ok_Click()

For i = 1 To numclass
    If highclass(List1.ListIndex + 1) < lowclass(List1.ListIndex + 1) Then
        X = MsgBox("The upper class limit must be larger than the lower class limit.  Choose again.", 0, "Error!")
        user.Show
    End If
Next

List1.Clear
user.Hide

For i = 1 To n
    For j = 1 To numclass
        If value(i) >= lowclass(j) And value(i) <= highclass(j) Then cl(i) = j
        If value(i) >= lowclass(j) And value(i) <= highclass(j) Then nclass(j) = nclass(j) + 1
    Next

Next
    
    'report classing results**************

classer.Hide
reporter.List1.Clear
reporter.List2.Clear
reporter.List3.Clear
reporter.List4.Clear
reporter.List5.Clear

reporter.Show
reporter.Label1.Caption = dataname$
reporter.Label2.Caption = Text2.Text

For i = 1 To n
    reporter.List1.AddItem nam$(i) + Str$(cl(i))
Next


For i = 1 To numclass
    reporter.List2.AddItem Str$(i)
    reporter.List3.AddItem Str$(lowclass(i))
    reporter.List4.AddItem Str$(highclass(i))
    reporter.List5.AddItem Str$(nclass(i))
Next


End Sub

Private Sub Text1_Change()

lowclass(List1.ListIndex + 1) = Val(Text1.Text)

End Sub

Private Sub Text2_Change()

highclass(List1.ListIndex + 1) = Val(Text2.Text)

End Sub

