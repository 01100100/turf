Attribute VB_Name = "grapher"
Attribute VB_Creatable = False
Attribute VB_Exposed = False
Private Sub begin_Click()

xsca = (grapher.Line2.X2 - grapher.Line2.X1) / n
ysca = (grapher.Line1.Y2 - grapher.Line1.Y1) / (maxvalue - minvalue)
For i = 1 To n
    X = grapher.Line2.X1 + Int(xsca * i)
    Y = grapher.Line1.Y2 - Int(ysca * value(i))
    grapher.Picture1.Circle (X, Y), 30, QBColor(4)
Next
If check = 1 Then
    For i = 1 To numclass - 1
        x11 = grapher.Line2.X1
        x22 = grapher.Line2.X2
        Y = grapher.Line1.Y2 - Int(ysca * highclass(i))
        grapher.Picture1.Line (x11, Y)-(x22, Y), QBColor(4)
    Next
End If
End Sub

Private Sub ok_Click()

grapher.Hide
If check = 0 Then
    intro.Show
Else
    reporter.Show
End If

End Sub

